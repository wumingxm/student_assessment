<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://www.mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.student_assessment.dao.AssessScoreTabDao">
	<insert id="addAssessScoreTab" parameterType="AssessScoreTab">
		insert into ASSESS_SCORE_TAB (assessItemId,assessItemGrade,assessTabId)
		values(#{assessItemId},#{assessItemGrade},#{assessTabId})
	</insert>
</mapper>